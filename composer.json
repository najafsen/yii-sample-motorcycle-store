{
  "repositories": [
      {
          "type":"composer",
          "url": "http://packages.phundament.com"
      }
  ],
  "require": {
      "php": ">=5.3.2",
      "yiisoft/yii": "1.1.*",
      "yiiext/migrate-command": "0.7.*"
  },
  "require-dev": {
    "phpunit/php-invoker": "*",
    "phpunit/dbunit": ">=1.2",
    "phpunit/phpunit-selenium": ">=1.2",
    "phpunit/phpunit-story": "*"
  },
  "config": {
      "bin-dir": "bin/",
      "secure-http": false
  },
  "autoload": {
      "psr-0": {
          "config": "./"
      }
  }
}